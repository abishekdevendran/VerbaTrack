{
	"name": "verbatrack",
	"version": "0.0.1",
	"description": "Backend package for Verbatrack",
	"main": "index.js",
	"scripts": {
		"build": "node config/esbuild.js",
		"start": "node dist/build.js",
		"dev": "nodemon - exec 'ts-node' src/index.ts",
		"db:generate": "drizzle-kit generate:pg --schema=./src/databases/postgres/schema.ts",
		"db:migrate": "ts-node -r dotenv/config ./src/databases/postgres/migrate.ts",
		"db:push": "drizzle-kit push:pg --config ./src/databases/postgres/drizzle.config.ts",
		"db:studio": "drizzle-kit studio --port 5001 --config ./src/databases/postgres/drizzle.config.ts"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^5.1.1",
		"connect-redis": "^7.1.0",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.28.2",
		"drizzle-zod": "^0.5.0",
		"express": "^4.18.2",
		"express-session": "^1.17.3",
		"helmet": "^7.0.0",
		"morgan": "^1.10.0",
		"nodemon": "^3.0.1",
		"passport": "^0.6.0",
		"passport-github2": "^0.1.12",
		"passport-local": "^1.0.0",
		"postgres": "^3.3.5",
		"redis": "^4.6.7",
		"save-dev": "^0.0.1-security",
		"ts-node": "^10.9.1",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@swc/core": "^1.3.78",
		"@swc/helpers": "^0.5.1",
		"@types/bcrypt": "^5.0.0",
		"@types/cookie-parser": "^1.4.3",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/express-session": "^1.17.7",
		"@types/morgan": "^1.9.4",
		"@types/node": "^20.5.1",
		"@types/passport": "^1.0.12",
		"@types/passport-github2": "^1.2.5",
		"@types/passport-local": "^1.0.35",
		"drizzle-kit": "^0.19.13",
		"esbuild": "^0.19.2",
		"pg": "^8.11.3",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.1.6"
	}
}
